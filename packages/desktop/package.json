    "electron-rebuild": "^1.7.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "null-loader": "^0.1.1",
    "style-loader": "^0.21.0",
    "ts-loader": "3.x.x",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "3.x.x",
    "webpack-dev-server": "2.x.x",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.6.0"
  }
