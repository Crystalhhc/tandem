{
  "name": "tandem-desktop",
  "version": "10.0.36",
  "description": "",
  "private": true,
  "main": "lib/index.js",
  "bin": {
    "tandem": "./bin/tandem"
  },
  "scripts": {
    "start": "electron lib/index.js",
    "postinstall": "electron-rebuild",
    "build": "tsc; webpack",
    "build-watch": "tsc; webpack --watch",
    "build-front-end": "webpack",
    "clean": "rm -rf lib; rm -rf node_modules",
    "prepare": "npm run build",
    "build-dist": "electron-packager . Tandem --out=dist --overwrite --icon=assets/tandem.icns"
  },
  "build": {
    "appId": "com.tandemcode.app",
    "fileAssociations": [
      {
        "ext": "tdproject",
        "name": "Tandem Project",
        "role": "Editor"
      }
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^2.0.4",
    "crc32": "^0.2.2",
    "font-manager": "^0.3.0",
    "fs-extra": "^6.0.1",
    "fsbox": "^10.0.36",
    "get-port": "^3.2.0",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.20",
    "paperclip": "^10.0.36",
    "paperclip-migrator": "^10.0.36",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "tandem-common": "^10.0.36",
    "tandem-front-end": "^10.0.36"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.2",
    "css-loader": "^0.28.11",
